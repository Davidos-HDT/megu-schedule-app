{
  "cli": {
    "version": ">= 0.49.0"
  },
  "build": {
    "development": {
      "releaseChannel": "dev",
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "development"
      }
    },
    "preview": {
      "releaseChannel": "staging",
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "development"
      }
    },
    "production": {
      "releaseChannel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
